        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WnsPushNotificationClient_NET50"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="RegisterForPushNotificationsButton" Content="Register for Push Notifications" HorizontalAlignment="Center" Margin="0,163,0,0" Padding="10,10,10,10" VerticalAlignment="Top" Click="RegisterForPushNotificationsButton_Click"/>
        <TextBlock HorizontalAlignment="Left" Margin="226,240,0,0" Text="WNS Push Notification Channel Uri:"/>
        <TextBox x:Name="ChannelUriTextBox" HorizontalAlignment="Center" Margin="0,262,0,0" Text="" IsReadOnly="True" TextWrapping="NoWrap" VerticalAlignment="Top" Width="348"/>
    </Grid>
</Window>
